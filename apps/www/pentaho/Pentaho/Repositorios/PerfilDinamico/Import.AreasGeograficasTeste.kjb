      <type>SQL</type>
      <sql>--fazendo o calculo de todas as combina&#xe7;&#xf5;es de agrega&#xe7;&#xf5;es poss&#xed;veis&#xd;&#xa;&#xd;&#xa;truncate table geografia_teste.territorio_agregacao_total&#x3b;&#xd;&#xa;&#xd;&#xa;WITH RECURSIVE regiao_superior&#x28;regiao, territorio, regiao_superior, territorio_superior&#x29; AS &#x28;&#xd;&#xa;		    select distinct cod_regiao,cod_territorio, cod_regiao_superior, cod_territorio_superior from geografia_teste.territorio_agregacao &#xd;&#xa;		  UNION ALL&#xd;&#xa;		    SELECT regiao,territorio,cod_regiao_superior,cod_territorio_superior FROM regiao_superior sup &#xd;&#xa;		    inner join &#x28;select cod_regiao,cod_territorio,cod_regiao_superior,cod_territorio_superior from geografia_teste.territorio_agregacao&#x29; agr on sup.regiao_superior &#x3d; agr.cod_regiao and agr.cod_territorio &#x3d; sup.territorio_superior&#xd;&#xa;		  &#x29;&#xd;&#xa;	insert into geografia_teste.territorio_agregacao_total&#xd;&#xa;	select &#x2a; from regiao_superior&#x3b;</sql>
